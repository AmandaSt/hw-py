import csv

with open('people.csv', 'r') as csv_file:
    csv_reader = csv.DictReader(csv_file)

    line_count = 0

    for row in csv_reader:
        if line_count == 0:
            print(f'{""}')
            line_count += 1

        print(f'{row["Name"]} is {row["Gender"]} and {row["Age"]} years old.')
        line_count += 1
